    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "vite": "^5.2.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
